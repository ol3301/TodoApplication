<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:Todo.Desktop.Features.Todo.TodoCommandPanel"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Todo.Desktop.Features.Todo.TodoCommandPanel.TodoCommandPanelView"
             x:DataType="local:TodoCommandPanelViewModel"
             x:CompileBindings="True"
             >
    <StackPanel Orientation="Horizontal">
        <Button Command="{Binding HomeCommand}">Home</Button>
        <Button Command="{Binding AddCommand}">Add</Button>
        <Button Command="{Binding DeleteCommand}">Delete</Button>
        <Button Command="{Binding EditCommand}">Edit</Button>
    </StackPanel>
</UserControl>
